#include<iostream>
using namespace std;
class space{
    int x;
    int y;
    int z;
    public:
    void getdata(int z1,int z2,int z3);
    void display(void);
    void operator-();
};
void space::getdata(int z1,int z2,int z3)
{
    x=z1;
    y=z2;
    z=z3;
}
void space::display(void)
{
    cout<<x<<"";
    cout<<y<<"";
    cout<<z<<"\n";
}
void space::operator-()
{
    x=-x;
    y=-y;
    z=-z;
}
int main()
{
    space s;
    s.getdata(30,-20,10);
    cout<<"s"<<endl;
    s.display();
    -s;
    cout<<"s";
    s.display();
    return 0;
}
