#include<iostream>
using namespace std;
class num1;
class num2{
    private:
    int b;
    public:
    void setb(int val)
    {
        b=val;
    }
    friend void number(num1,num2);
};
class num1{
    private:
    int a;
    public:
    void seta(int val)
    {
        a=val;
    }
    friend void number(num1,num2);
};
void number(num1 n1,num2 n2)
{
    if(n1.a>n2.b)
    {
        cout<<"the A is greater :"<<n1.a<<endl;
    }
    else
    {
    if(n1.a<n2.b)
    {
       cout<<"the B is greater :"<<n2.b<<endl; 
    }
    else
    {
        cout<<"the A and B is equal : "<<n1.a<<"="<<n2.b<<endl;
    }
    }
}
int main()
{
    num1 a1;
    num2 a2;
    int o1,o2;
    cout<<"enter the value of A :";
    cin>>o1;
    a1.seta(o1);
    cout<<"enter the value of B :";
    cin>>o2;
    a2.setb(o2);
    number(a1,a2);
    return 0;
}
