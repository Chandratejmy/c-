#include<iostream>
using namespace std;
class B
{
    int a;
    public:
    int b;
    void get_ab();
    int get_a();
    void show_c();
};
void B::get_ab()
{
    cout<<"enter the A & B :"<<endl;
    cin>>a>>b;
}
int B::get_a()
{
    return a;
}
void B::show_c()
{
    cout<<"a="<<a<<"\n";
}
class D:public B
{
    int c;
    public:
    void mul():
    void display();
};
void D::mul()
{
    c=b*get_a();
}
void D::display()
{
    cout<<"a :"<<geta()<<endl;
    cout<<"b :"<<b<<endl;
    cout<<"C :"<<c<<endl;
}
int main()
{
    D d;
    d.get_ab();
    d.mul();
    d.display();
    return 0;
}
