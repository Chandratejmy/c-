#include<iostream>
using namespace std;
class M
{
    public:
    void display()
    {
        cout<<"class M\n";
    }
};
class N
{
    public:
    void display()
    {
        cout<<"class N\n";
    }
};
class P:public M,public N
{
    public:
    void print()
    {
        M::display();
    }
};
int main()
{
    P p;
    p.M::display();
}
